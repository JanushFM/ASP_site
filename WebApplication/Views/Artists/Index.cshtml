@model IEnumerable<Domain.Entities.Artist>
@{
    ViewData["Title"] = "Artists";
}

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(modelItem => modelItem.Quote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description.SmallDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description.BigDescription)
        </th>
    </tr>
    </thead>
    <tbody>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description.SmallDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description.BigDescription)
            </td>

        </tr>
    }
    </tbody>
</table>